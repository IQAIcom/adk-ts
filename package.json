{
	"name": "@iqai/adk",
	"version": "0.0.1",
	"description": "Agent Development Kit for TypeScript with multi-provider LLM support",
	"main": "dist/index.js",
	"types": "dist/index.d.mjs",
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"format": "biome format . --write",
		"lint": "lint-staged --allow-empty",
		"lint:all": "biome check . --write --organize-imports-enabled=true",
		"clean": "rimraf dist",
		"prepare": "pnpm run build",
		"docs": "typedoc --out docs src/index.ts || exit 0",
		"docs:serve": "npx http-server docs",
		"docs:markdown": "typedoc --plugin typedoc-plugin-markdown",
		"predeploy": "npm run docs",
		"deploy:docs": "gh-pages -d docs",
		"example": "tsx examples/index.ts",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"release": "pnpm build && changeset publish"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/IQAIcom/adk-ts.git"
	},
	"keywords": [
		"ai",
		"llm",
		"agent",
		"openai",
		"anthropic",
		"gemini",
		"typescript"
	],
	"author": "IQAI",
	"license": "MIT",
	"dependencies": {
		"@anthropic-ai/sdk": "^0.39.0",
		"@clack/prompts": "^0.10.1",
		"@electric-sql/pglite": "^0.3.2",
		"@google-cloud/vertexai": "^0.5.0",
		"@google/genai": "^1.0.0",
		"@modelcontextprotocol/sdk": "^1.11.1",
		"axios": "^1.6.2",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.43.1",
		"openai": "^4.93.0",
		"uuid": "^11.1.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.2",
		"@biomejs/biome": "1.9.4",
		"@types/node": "^20.17.30",
		"gh-pages": "^6.3.0",
		"lint-staged": "^15.5.2",
		"prettier": "^3.1.0",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.4",
		"tsup": "^8.4.0",
		"typedoc": "^0.28.2",
		"typedoc-plugin-markdown": "^4.6.2",
		"typescript": "^5.3.2",
		"vitest": "^3.1.3"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=22.0"
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.json",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --write --organize-imports-enabled=false --no-errors-on-unmatched"
		]
	},
	"publishConfig": {
		"access": "public"
	}
}
