{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["../../packages/adk/src/index.ts"],
	"out": "./api",
	"theme": "default",
	"name": "@iqai/adk API Documentation",
	"readme": "../../packages/adk/README.md",
	"tsconfig": "../../packages/adk/tsconfig.json",
	"excludePrivate": true,
	"excludeProtected": false,
	"excludeExternals": true,
	"hideGenerator": true,
	"sort": ["source-order"],
	"kindSortOrder": [
		"Project",
		"Module",
		"Namespace",
		"Enum",
		"Enumeration Member",
		"Class",
		"Interface",
		"Type alias",
		"Constructor",
		"Property",
		"Variable",
		"Function",
		"Accessor",
		"Method",
		"Parameter",
		"Other Kind",
		"Type literal",
		"Type parameter",
		"Reference"
	],
	"categorizeByGroup": true,
	"defaultCategory": "Other",
	"categoryOrder": [
		"Agents",
		"Models",
		"Tools",
		"Memory",
		"Sessions",
		"Events",
		"Flows",
		"Evaluation",
		"Auth",
		"Utils",
		"*",
		"Other"
	],
	"searchInComments": true,
	"searchInDocuments": true,
	"validation": {
		"notExported": true,
		"invalidLink": true,
		"notDocumented": false
	},
	"treatWarningsAsErrors": false,
	"intentionallyNotExported": [],
	"plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter"],
	"gitRevision": "main",
	"gitRemote": "origin",
	"githubPages": true,
	"disableSources": false,
	"sourceLinkTemplate": "https://github.com/IQAIcom/adk-ts/blob/{gitRevision}/{path}#L{line}",
	"customCss": "./_includes/custom.css"
}
