{
	"name": "@iqai/adk-cli",
	"version": "0.1.0",
	"description": "CLI tool for creating, running, and testing ADK agents",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"adk": "./dist/cli/index.js"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/IQAIcom/adk-ts.git",
		"directory": "packages/adk-cli"
	},
	"keywords": ["ai", "llm", "agent", "cli", "adk", "typescript"],
	"author": "IQAI",
	"license": "MIT",
	"dependencies": {
		"@iqai/adk": "workspace:*",
		"@clack/prompts": "^0.11.0",
		"chalk": "^5.4.1",
		"chokidar": "^4.0.3",
		"commander": "^12.1.0",
		"express": "^4.19.2",
		"cors": "^2.8.5",
		"socket.io": "^4.8.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"giget": "^2.0.0",
		"dedent": "^1.6.0"
	},
	"devDependencies": {
		"@iqai/tsconfig": "workspace:*",
		"@types/express": "^4.17.21",
		"@types/cors": "^2.8.19",
		"@types/node": "^20.17.30",
		"tsup": "^8.4.0",
		"typescript": "^5.3.2",
		"vitest": "^3.1.3"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=22.0"
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.json",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	}
}
