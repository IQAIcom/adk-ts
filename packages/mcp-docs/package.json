{
	"name": "@iqai/mcp-docs",
	"version": "0.1.0",
	"description": "MCP server for accessing ADK-TS documentation, examples, API reference, and MCP server integrations.",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": "dist/stdio.js",
	"files": [
		"dist",
		".docs",
		"README.md"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "tsx src/index.ts",
		"build": "tsc && npm run copy-docs",
		"copy-docs": "mkdir -p .docs && cp -r ../../apps/docs/content/docs/* .docs/",
		"start": "node dist/index.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"keywords": [
		"iqai",
		"mcp",
		"adk",
		"adk-ts",
		"ai-agents",
		"documentation",
		"model-context-protocol"
	],
	"author": "IQ AI",
	"license": "Apache-2.0",
	"packageManager": "pnpm@10.28.1",
	"dependencies": {
		"@iqai/adk": "0.6.2",
		"@modelcontextprotocol/sdk": "^1.25.3",
		"@openrouter/ai-sdk-provider": "1.2.0",
		"fast-glob": "^3.3.3",
		"fastmcp": "^3.30.1",
		"gray-matter": "^4.0.3",
		"marked": "^17.0.1",
		"winston": "^3.19.0",
		"zod": "^3.25.0"
	},
	"devDependencies": {
		"@types/node": "^25.0.10",
		"ts-node": "^10.9.2",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^3.0.0"
	},
	"homepage": "https://adk.iqai.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/IQAIcom/adk-ts.git",
		"directory": "packages/mcp-docs"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
