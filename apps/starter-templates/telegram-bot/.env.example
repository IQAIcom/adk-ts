# ADK framework debug logs (optional)
ADK_DEBUG=false

# LLM provider API key (required)
# Get a key from https://aistudio.google.com/api-keys
GOOGLE_API_KEY=your_google_api_key_here

# LLM model name (optional)
LLM_MODEL=gemini-2.5-flash

# Telegram platform settings
# Create a bot and token with https://t.me/botfather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
