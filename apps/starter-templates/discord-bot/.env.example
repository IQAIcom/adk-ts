# ADK framework debug logs (optional)
ADK_DEBUG=false

# LLM provider API key (required)
# Get a key from https://aistudio.google.com/api-keys
GOOGLE_API_KEY=your_google_api_key_here

# LLM model name (optional)
LLM_MODEL=gemini-2.5-flash

# Discord platform settings
# Create an app and bot token at https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here
