# ADK-TS framework debug logs (optional)
ADK_DEBUG=false

# LLM provider API key (required)
# Get a key from https://aistudio.google.com/api-keys
GOOGLE_API_KEY=your_google_api_key_here

# LLM model name (optional)
LLM_MODEL=gemini-2.5-flash

# NEAR / Shade platform settings
# Create a testnet account at https://testnet.mynearwallet.com/
NEAR_ACCOUNT_ID=your-name.testnet
# Export from your NEAR account credentials (12 words)
NEAR_SEED_PHRASE=your_twelve_word_seed_phrase
# Contract account id created for your shade app
NEXT_PUBLIC_contractId=ac-sandbox.your-name.testnet
# RPC endpoint for testnet
NEAR_RPC_JSON=https://rpc.testnet.near.org

# Phala settings
# Create an API key at https://cloud.phala.network/
PHALA_API_KEY=your_phala_api_key_here

# Deployment metadata
DOCKER_TAG=your-dockerhub-username/shade-agent
API_CODEHASH=your_api_codehash_here
APP_CODEHASH=your_app_codehash_here
