{
	"name": "@iqai/adk",
	"version": "0.0.12",
	"description": "Agent Development Kit for TypeScript with multi-provider LLM support",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/IQAIcom/adk-ts.git"
	},
	"keywords": [
		"ai",
		"llm",
		"agent",
		"openai",
		"anthropic",
		"gemini",
		"typescript"
	],
	"author": "IQAI",
	"license": "MIT",
	"dependencies": {
		"@anthropic-ai/sdk": "^0.39.0",
		"@electric-sql/pglite": "^0.3.2",
		"@google-cloud/vertexai": "^0.5.0",
		"@google/genai": "^1.0.0",
		"@modelcontextprotocol/sdk": "^1.11.1",
		"axios": "^1.6.2",
		"better-sqlite3": "^11.10.0",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.43.1",
		"openai": "^4.93.0",
		"uuid": "^11.1.0",
		"zod": "^3.24.4"
	},
	"devDependencies": {
		"@iqai/tsconfig": "workspace:*",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^20.17.30",
		"tsup": "^8.4.0",
		"typescript": "^5.3.2",
		"vitest": "^3.1.3"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=22.0"
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.json",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	}
}
