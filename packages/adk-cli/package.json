{
	"name": "@iqai/adk-cli",
	"version": "0.2.9",
	"description": "CLI tool for creating, running, and testing ADK agents",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"bin": {
		"adk": "./dist/main.js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsc -w -p tsconfig.json",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/IQAIcom/adk-ts.git",
		"directory": "packages/adk-cli"
	},
	"keywords": ["ai", "llm", "agent", "cli", "adk", "typescript"],
	"author": "IQAI",
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"@iqai/adk": "workspace:*",
		"chalk": "^5.4.1",
		"dedent": "^1.6.0",
		"giget": "^2.0.0",
		"marked": "^14.1.3",
		"marked-terminal": "^7.2.1",
		"esbuild": "^0.23.0",
		"@nestjs/common": "^10.4.0",
		"@nestjs/core": "^10.4.0",
		"@nestjs/platform-express": "^10.4.0",
		"@nestjs/swagger": "^7.4.2",
		"swagger-ui-express": "^5.0.1",
		"nest-commander": "^3.4.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"class-validator": "^0.14.1",
		"class-transformer": "^0.5.1"
	},
	"devDependencies": {
		"@iqai/tsconfig": "workspace:*",
		"@types/marked-terminal": "^6.1.1",
		"@types/node": "^20.17.30",
		"typescript": "^5.3.2",
		"vitest": "^3.1.3"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=22.0"
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.json",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	}
}
